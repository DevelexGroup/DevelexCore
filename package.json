{
	"name": "develex-js-sdk",
	"version": "0.3.7",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "vite build --mode lib && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.7",
		"@types/node": "^20.16.12",
		"dexie": "^4.0.8",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.4.9",
		"vite-plugin-dts": "^3.9.1",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/develex-js-sdk.umd.cjs",
	"module": "./dist/develex-js-sdk.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/develex-js-sdk.js",
			"require": "./dist/develex-js-sdk.umd.cjs"
		}
	},
	"dependencies": {
		"vite-plugin-pwa": "^0.21.1"
	}
}
