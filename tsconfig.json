{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2017",
		"moduleResolution": "node",
		"types": ["svelte", "vite/client", "@sveltejs/kit"]
	},
	"include": [
		"src/**/*",
		"src/app.d.ts"
	]
}
  